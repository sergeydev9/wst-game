{
  "npmScope": "whosaidtrue",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": []
    },
    "api-interfaces": {
      "tags": []
    },
    "app-interfaces": {
      "tags": []
    },
    "csr-frontend": {
      "tags": []
    },
    "csr-frontend-e2e": {
      "tags": [],
      "implicitDependencies": ["csr-frontend"]
    },
    "data": {
      "tags": []
    },
    "database": {
      "tags": []
    },
    "email": {
      "tags": []
    },
    "logger": {
      "tags": []
    },
    "middleware": {
      "tags": []
    },
    "socket-server": {
      "tags": ["socket.io server"]
    },
    "static-frontend": {
      "tags": ["static"]
    },
    "static-frontend-e2e": {
      "tags": [],
      "implicitDependencies": ["static-frontend"]
    },
    "ui": {
      "tags": ["ui"]
    },
    "util": {
      "tags": []
    },
    "validation": {
      "tags": []
    }
  }
}
